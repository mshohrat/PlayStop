    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">
    <font
        app:font="@font/iran_sans_bold"
        app:fontWeight="700"
        android:font="@font/iran_sans_bold"
